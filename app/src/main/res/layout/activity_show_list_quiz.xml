<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.ShowListQuizActivity">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/showlistquiztoolbar"
        app:navigationIcon="@drawable/thong_back_white"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#00587A"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Principle programming language"
        app:titleTextColor="#fff" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycle_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="0dp"
        android:padding="20dp"
        app:layout_constraintTop_toBottomOf="@id/showlistquiztoolbar"
        tools:layout_editor_absoluteX="0dp"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        tools:listitem="@layout/layout_listquiz"/>
</androidx.constraintlayout.widget.ConstraintLayout>