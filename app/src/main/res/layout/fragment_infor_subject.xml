    <View
        android:id="@+id/view"
        android:layout_width="8dp"
        android:layout_height="320dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/thong_rectangle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/compiler1"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/compiler1"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/thong_compiler1"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/view8"
        android:layout_width="100dp"
        android:layout_height="5dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/thong_rectangle"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/compiler1" />

    <View
        android:id="@+id/view9"
        android:layout_width="100dp"
        android:layout_height="5dp"
        android:layout_marginTop="63dp"
        android:background="@drawable/thong_rectangle"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/view8" />

    <View
        android:id="@+id/view10"
        android:layout_width="100dp"
        android:layout_height="5dp"
        android:layout_marginTop="63dp"
        android:background="@drawable/thong_rectangle"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/view9" />

    <View
        android:id="@+id/view11"
        android:layout_width="100dp"
        android:layout_height="5dp"
        android:layout_marginTop="56dp"
        android:background="@drawable/thong_rectangle"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/view10" />

    <View
        android:id="@+id/view12"
        android:layout_width="100dp"
        android:layout_height="5dp"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/thong_rectangle"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/view11" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/view8"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:srcCompat="@drawable/thong_icon_book1" />

    <TextView
        android:id="@+id/idSubject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="7dp"
        android:fontFamily="@font/roboto_bold"
        android:text="CO3005"
        android:textColor="#00587A"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/view8"
        app:layout_constraintStart_toEndOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/view9"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:srcCompat="@drawable/thong_icon_class_open_file" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/view10"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:srcCompat="@drawable/thong_icon_presentation" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/view11"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:srcCompat="@drawable/thong_icon_back_in_time" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@+id/view12"
        app:layout_constraintStart_toEndOf="@+id/view"
        app:srcCompat="@drawable/thong_icon_teacher_at_the_blackboard" />

    <TextView
        android:id="@+id/classID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="7dp"
        android:fontFamily="@font/roboto_bold"
        android:text="L02"
        android:textColor="#00587A"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/view9"
        app:layout_constraintStart_toEndOf="@+id/imageView2" />

    <TextView
        android:id="@+id/room"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="7dp"
        android:fontFamily="@font/roboto_bold"
        android:text="H6 - 301"
        android:textColor="#00587A"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/view10"
        app:layout_constraintStart_toEndOf="@+id/imageView3" />

    <TextView
        android:id="@+id/SEtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="7dp"
        android:fontFamily="@font/roboto_bold"
        android:text="7:00 - 9:50"
        android:textColor="#00587A"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/view11"
        app:layout_constraintStart_toEndOf="@+id/imageView4" />

    <TextView
        android:id="@+id/nameTeacher"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="7dp"
        android:fontFamily="@font/roboto_bold"
        android:text="Nguyen Hua Phung"
        android:textColor="#00587A"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/view12"
        app:layout_constraintStart_toEndOf="@+id/imageView5" />


</androidx.constraintlayout.widget.ConstraintLayout>